#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules/@antfu/ni/bin/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules/@antfu/ni/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules/@antfu/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules/@antfu/ni/bin/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules/@antfu/ni/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules/@antfu/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/@antfu+ni@0.21.12/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@antfu+ni@0.21.12/node_modules/@antfu/ni/bin/ni.mjs" "$@"
else
  exec node  "$basedir/../../../../../@antfu+ni@0.21.12/node_modules/@antfu/ni/bin/ni.mjs" "$@"
fi
