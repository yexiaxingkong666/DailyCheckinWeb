#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules/vite/bin/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules/vite/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules/vite/bin/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules/vite/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@5.2.10_@types+node@20.12.7_lightningcss@1.24.1_terser@5.31.0/node_modules/vite/bin/vite.js" "$@"
fi
