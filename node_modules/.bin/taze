#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/taze@0.13.7/node_modules/taze/bin/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/taze@0.13.7/node_modules/taze/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/taze@0.13.7/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/taze@0.13.7/node_modules/taze/bin/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/taze@0.13.7/node_modules/taze/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/taze@0.13.7/node_modules:/mnt/i/vuejsProjects/tov-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../taze/bin/taze.mjs" "$@"
else
  exec node  "$basedir/../taze/bin/taze.mjs" "$@"
fi
